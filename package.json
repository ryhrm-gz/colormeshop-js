{
  "name": "colormeshop-js",
  "description": "JavaScript client for ColorMeShop API",
  "version": "0.4.1",
  "author": "ryhrm-gz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryhrm-gz/colormeshop-js.git"
  },
  "keywords": [
    "JavaScript",
    "TypeScript",
    "Colorme"
  ],
  "main": "./dist/colormeshop-js.umd.js",
  "module": "./dist/colormeshop-js.es.js",
  "exports": {
    ".": {
      "import": "./dist/colormeshop-js.es.js",
      "require": "./dist/colormeshop-js.umd.js"
    }
  },
  "types": "./types/index.d.ts",
  "files": [
    "/dist",
    "/types"
  ],
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "generate": "openapi-typescript https://api.shop-pro.jp/v1/spec/open_api.json -o ./src/generated/types.gen.ts"
  },
  "devDependencies": {
    "@types/node": "^20.11.17",
    "openapi-typescript": "^6.7.4",
    "typescript": "^5.3.3"
  }
}
